
@{
    ViewBag.Title = "AddTypePhone";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}


<link href="~/Assets/Admin/css/scud.css" rel="stylesheet" />
    <script src="~/Assets/Admin/script/angular.min.js"></script>
    <script src="~/Assets/Admin/script/AngularController/HomeController.js"></script>


    <div class="back-add-type" ng-controller="AddTypePhone">
        <div class="import-product-type">
            <table style="width:100%;text-align:center">
                <tr class="row-list">
                    <td>Thông tin</td>
                    <td><a href="/Admin/ManagerTypePhone/ManagerTypePhone">Quay lại</a></td>
                </tr>
                <tr class="row-list">
                    <td class="lable-product-type">Tên loại sản phẩm</td>
                    <td>
                        <input type="text" class="product-type-input" id="name_type" name="name" ng-model="LoaiDT.TenLoai">
                    </td>
                </tr>
                <tr class="row-list">
                    <td class="lable-product-type">Xuất xứ</td>
                    <td>
                        <input type="text" class="product-type-input" id="description_type" name="origin" ng-model="LoaiDT.XuatXu">
                    </td>
                </tr>
                <tr>
                    <td>
                    <input type="button" value="{{btntextadd}}" ng-click="savedata()" />
                    </td>
                    <td></td>
                </tr>
            </table>
        </div>
    </div>


